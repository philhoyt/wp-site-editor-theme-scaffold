{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "settings": {
    "appearanceTools": true,
    "color": {
      "custom": true,
      "customDuotone": false,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "palette": [
        {
          "color": "#ffffff",
          "name": "Base",
          "slug": "base"
        },
        {
          "color": "#000000",
          "name": "Contrast",
          "slug": "contrast"
        },
        {
          "color": "#F24C62",
          "name": "Primary",
          "slug": "primary"
        },
        {
          "color": "#49C6DD",
          "name": "Secondary",
          "slug": "secondary"
        },
        {
          "color": "#005070",
          "name": "Tertiary",
          "slug": "tertiary"
        },
        {
          "color": "#434344",
          "name": "Contrast - Dark",
          "slug": "contrast-dark"
        },
        {
          "color": "#C7C4C4",
          "name": "Contrast - Medium",
          "slug": "contrast-medium"
        },
        {
          "color": "#F7F6F6",
          "name": "Contrast - Light",
          "slug": "contrast-light"
        }
      ]
    },
    "custom": {
      "lineHeight": {
        "large": "2.2",
        "medium": "1.8",
        "small": "1.5",
        "x-large": "2.6"
      },
      "outline": {
        "width": "2px",
        "offset": "2px",
        "style": "solid"
      }
    },
    "layout": {
      "contentSize": "960px",
      "wideSize": "1440px"
    },
    "border": {
      "radius": true,
      "radiusSizes": [
        {
          "name": "XS",
          "slug": "xs",
          "size": "0.125rem"
        },
        {
          "name": "Small",
          "slug": "sm",
          "size": "0.25rem"
        },
        {
          "name": "Medium",
          "slug": "md",
          "size": "0.375rem"
        },
        {
          "name": "Large",
          "slug": "lg",
          "size": "0.5rem"
        },
        {
          "name": "XL",
          "slug": "xl",
          "size": "0.75rem"
        },
        {
          "name": "2XL",
          "slug": "2-xl",
          "size": "1rem"
        },
        {
          "name": "3XL",
          "slug": "3-xl",
          "size": "1.5rem"
        }
      ]
    },
    "spacing": {
      "customSpacingSize": true,
      "defaultSpacingSizes": false,
      "spacingSizes": [
        {
          "name": "3X-Small",
          "size": "0.25rem",
          "slug": "10"
        },
        {
          "name": "2X-Small",
          "size": "0.5rem",
          "slug": "20"
        },
        {
          "name": "X-Small",
          "size": "0.75rem",
          "slug": "30"
        },
        {
          "name": "Small",
          "size": "1rem",
          "slug": "40"
        },
        {
          "name": "Medium",
          "size": "1.5rem",
          "slug": "50"
        },
        {
          "name": "Large",
          "size": "clamp(1.75rem, 4vw, 2.5rem)",
          "slug": "60"
        },
        {
          "name": "X-Large",
          "size": "clamp(2.5rem, 5vw, 3rem)",
          "slug": "70"
        },
        {
          "name": "2X-Large",
          "size": "clamp(3rem, 6vw, 4rem)",
          "slug": "80"
        },
        {
          "name": "3X-large",
          "size": "clamp(4rem, 8vw, 6rem)",
          "slug": "90"
        }
      ],
      "units": ["rem", "em", "%", "vw", "vh", "px"]
    },
    "typography": {
      "customFontSize": true,
      "defaultFontSizes": false,
      "dropCap": true,
      "fluid": true,
      "fontFamilies": [
        {
          "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
          "name": "System Font",
          "slug": "primary"
        }
      ],
      "fontSizes": [
        {
          "name": "3X-Small",
          "size": "0.625rem",
          "slug": "xxxs"
        },
        {
          "name": "2X-Small",
          "size": "0.75rem",
          "slug": "xxs"
        },
        {
          "name": "X-Small",
          "size": "0.875rem",
          "slug": "xs"
        },
        {
          "name": "Small",
          "size": "1rem",
          "slug": "s"
        },
        {
          "name": "Medium",
          "size": "1.125rem",
          "slug": "m"
        },
        {
          "fluid": {
            "max": "1.5rem",
            "min": "1.25rem"
          },
          "name": "Large",
          "size": "1.5rem",
          "slug": "l"
        },
        {
          "fluid": {
            "max": "2rem",
            "min": "1.5rem"
          },
          "name": "X-Large",
          "size": "2rem",
          "slug": "xl"
        },
        {
          "fluid": {
            "max": "2.25rem",
            "min": "1.75rem"
          },
          "name": "2X-Large",
          "size": "2.25rem",
          "slug": "xxl"
        },
        {
          "fluid": {
            "max": "3.75rem",
            "min": "2rem"
          },
          "name": "3X-Large",
          "size": "3.75rem",
          "slug": "xxxl"
        }
      ],
      "fontStyle": true,
      "fontWeight": true,
      "textDecoration": true,
      "textTransform": true
    },
    "useRootPaddingAwareAlignments": true
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--base)",
      "text": "var(--wp--preset--color--contrast)"
    },
    "blocks": {
      "core/columns": {
        "spacing": {
          "blockGap": "var:preset|spacing|50"
        }
      },
      "core/buttons": {
        "spacing": {
          "blockGap": "var:preset|spacing|40"
        }
      },
      "core/navigation": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)"
        },
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        }
      },
      "core/site-title": {
        "typography": {
          "fontWeight": "700"
        },
        "elements": {
          "link": {
            "typography": {
              "textDecoration": "none"
            },
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            }
          }
        }
      },
      "core/site-tagline": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)"
        }
      },
      "core/separator": {
        "border": {
          "color": "currentColor",
          "style": "solid",
          "width": "0 0 1px 0"
        },
        "color": {
          "text": "var(--wp--preset--color--contrast-medium)"
        }
      },
      "core/list": {
        "css": "& li { margin-top: 0.5rem; }"
      }
    },
    "elements": {
      "button": {
        "color": {
          "background": "var(--wp--preset--color--contrast)",
          "text": "var(--wp--preset--color--base)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--30)",
            "right": "var(--wp--preset--spacing--50)",
            "bottom": "var(--wp--preset--spacing--30)",
            "left": "var(--wp--preset--spacing--50)"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontWeight": "600"
        },
        ":hover": {
          "color": {
            "background": "var(--wp--preset--color--contrast-dark)",
            "text": "var(--wp--preset--color--base)"
          }
        },
        ":focus": {
          "color": {
            "background": "var(--wp--preset--color--contrast-dark)",
            "text": "var(--wp--preset--color--base)"
          },
          "outline": {
            "color": "var(--wp--preset--color--tertiary)",
            "offset": "var(--wp--custom--outline--offset)"
          }
        }
      },
      "caption": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "lineHeight": "1.4"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xxl)"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xl)"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--l)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--m)"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--s)",
          "letterSpacing": "0.5px"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--s)",
          "fontWeight": "700",
          "letterSpacing": "1px",
          "textTransform": "uppercase"
        }
      },
      "heading": {
        "typography": {
          "fontWeight": "400",
          "lineHeight": "1.2"
        }
      },
      "textInput": {
        "border": {
          "color": "var(--wp--preset--color--contrast-medium)",
          "radius": "0",
          "style": "solid",
          "width": "1px"
        },
        "color": {
          "background": "var(--wp--preset--color--base)",
          "text": "var(--wp--preset--color--contrast)"
        },
        "outline": {
          "color": "transparent",
          "offset": "var(--wp--custom--outline--offset)",
          "style": "var(--wp--custom--outline--style)",
          "width": "var(--wp--custom--outline--width)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--30)",
            "right": "var(--wp--preset--spacing--40)",
            "bottom": "var(--wp--preset--spacing--30)",
            "left": "var(--wp--preset--spacing--40)"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontFamily": "var(--wp--preset--font-family--primary)"
        },
        ":focus": {
          "outline": {
            "color": "var(--wp--preset--color--tertiary)",
            "offset": "var(--wp--custom--outline--offset)"
          }
        }
      },
      "select": {
        "border": {
          "color": "var(--wp--preset--color--contrast-medium)",
          "radius": "0",
          "style": "solid",
          "width": "1px"
        },
        "color": {
          "background": "var(--wp--preset--color--base)",
          "text": "var(--wp--preset--color--contrast)"
        },
        "outline": {
          "color": "transparent",
          "offset": "var(--wp--custom--outline--offset)",
          "style": "var(--wp--custom--outline--style)",
          "width": "var(--wp--custom--outline--width)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--30)",
            "right": "var(--wp--preset--spacing--40)",
            "bottom": "var(--wp--preset--spacing--30)",
            "left": "var(--wp--preset--spacing--40)"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontFamily": "var(--wp--preset--font-family--primary)"
        },
        ":focus": {
          "outline": {
            "color": "var(--wp--preset--color--tertiary)",
            "offset": "var(--wp--custom--outline--offset)"
          }
        }
      },
      "link": {
        ":focus": {
          "typography": {
            "textDecoration": "underline"
          }
        },
        ":hover": {
          "color": {
            "text": "inherit"
          },
          "typography": {
            "textDecoration": "underline"
          }
        },
        "color": {
          "text": "var(--wp--preset--color--contrast)"
        },
        "typography": {
          "textDecoration": "underline"
        }
      }
    },
    "spacing": {
      "blockGap": "1rem",
      "padding": {
        "bottom": "0",
        "left": "var:preset|spacing|50",
        "right": "var:preset|spacing|50",
        "top": "0"
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--primary)",
      "fontSize": "var(--wp--preset--font-size--medium)",
      "lineHeight": "var(--wp--custom--line-height--medium)"
    }
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header",
      "title": "Header"
    },
    {
      "area": "footer",
      "name": "footer",
      "title": "Footer"
    }
  ],
  "version": 3
}
