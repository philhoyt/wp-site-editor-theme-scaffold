{
	"$schema": "https://schemas.wp.org/wp/6.2/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": true,
			"defaultDuotone": false,
			"customDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#000000",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#F24C62",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#49C6DD",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#005070",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#434344",
					"name": "Contrast - Dark",
					"slug": "contrast-dark"
				},
				{
					"color": "#C7C4C4",
					"name": "Contrast - Medium",
					"slug": "contrast-medium"
				},
				{
					"color": "#F7F6F6",
					"name": "Contrast - Light",
					"slug": "contrast-light"
				}
			]
		},
		"custom": {
			"lineHeight": {
				"large": "2.2",
				"medium": "1.8",
				"small": "1.5",
				"x-large": "2.6"
			},
			"themeColors": {
				"primary": "#000",
				"secondary": "#000",
				"tertiary": "#000",
				"background": "#000",
				"foreground": "#000"
			}
		},
		"layout": {
			"contentSize": "40rem",
			"wideSize": "64rem"
		},
		"spacing": {
			"customSpacingSize": true,
			"spacingSizes": [
				{
					"name": "Tiny",
					"size": ".5rem",
					"slug": "tiny"
				},
				{
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "min(2rem, 6vw)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "min(4rem,8vw)",
					"slug": "large"
				},
				{
					"name": "Huge",
					"size": "min(6.75rem,10vw)",
					"slug": "huge"
				},
				{
					"name": "Gigantic",
					"size": "min(10rem,12vw)",
					"slug": "gigantic"
				}
			],
			"units": [
				"rem",
				"em",
				"%",
				"vw",
				"vh",
				"px"
			]
		},
		"typography": {
			"customFontSize": true,
			"dropCap": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "primary"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"max": "1rem",
						"min": "0.875rem"
					},
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.125rem",
						"min": "1rem"
					},
					"size": "1.125rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1.875rem",
						"min": "1.75rem"
					},
					"size": "1.75rem",
					"slug": "large"
				},
				{
					"fluid": false,
					"size": "2.25rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "10rem",
						"min": "4rem"
					},
					"size": "10rem",
					"slug": "xx-large"
				}
			],
			"fontStyle": true,
			"fontWeight": true,
			"textDecoration": true,
			"textTransform": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"spacing": {
			"blockGap": "1rem",
			"padding": {
				"bottom": "0",
				"left": "1.5rem",
				"right": "1.5rem",
				"top": "0"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "var(--wp--custom--line-height--medium)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"textDecoration": "underline"
				},
				":focus": {
					"typography": {
						"textDecoration": "underline"
					}
				},
				":hover": {
					"color": {
						"text": "inherit"
					},
					"typography": {
						"textDecoration": "underline"
					}
				}
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 2
}